string=input("Enter string:")
shree1=0
shree2=0
shree3=0
for i in string:
      if(i.islower()):
            shree1=shree1+1
      elif(i.isupper()):
            shree2=shree2+1
      elif (i.isdigit):
            shree3=shree3+1
print("The number of lowercase characters is:")
print(shree1)
print("The number of uppercase characters is:")
print(shree2)
print("The number of digits is:")
print(shree3)